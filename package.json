{
  "name": "nodeapi",
  "version": "1.0.0",
  "description": "A node JS API",
  "main": "app.js",
  "scripts": {
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node app.js",
    "build": "npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "node",
    "api"
  ],
  "author": "Adel Stiti",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-jwt": "^5.3.3",
    "express-validator": "^6.6.0",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "loadash": "^1.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.20",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.2",
    "nodemon": "^1.19.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
